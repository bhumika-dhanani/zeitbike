<?xml version="1.0" encoding="utf-8"?><config>    <modules>        <IWD_AlsoBought>            <version>1.2.0.0</version>        </IWD_AlsoBought>    </modules>    <global>        <blocks>            <iwd_alsobought>                <class>IWD_AlsoBought_Block</class>            </iwd_alsobought>        </blocks>        <helpers>            <iwd_alsobought>                <class>IWD_AlsoBought_Helper</class>            </iwd_alsobought>        </helpers>        <models>            <iwd_alsobought>                <class>IWD_AlsoBought_Model</class>            </iwd_alsobought>        </models>    </global>    <adminhtml>        <events>            <controller_action_predispatch>                <observers>                    <alsobought_check_related>                        <type>singleton</type>                        <class>iwd_alsobought/observer</class>                        <method>checkRequiredModules</method>                    </alsobought_check_related>                </observers>            </controller_action_predispatch>        </events>    </adminhtml>    <frontend>        <routers>            <iwd_alsobought>                <use>standard</use>                <args>                    <module>IWD_AlsoBought</module>                    <frontName>iwd_alsobought</frontName>                </args>            </iwd_alsobought>        </routers>        <layout>            <updates>                <iwd_alsobought>                    <file>iwdalsobought.xml</file>                </iwd_alsobought>            </updates>        </layout>    </frontend>    <default>        <iwd_alsobought>            <main_settings>                <order_statuses>complete,closed,pending,processing</order_statuses>                <sort_order_also_bought>1</sort_order_also_bought>                <sort_order_unpopular>1</sort_order_unpopular>                <store>1</store>                <category>1</category>                <child>0</child>                <unpopulars>crosssell,upsell</unpopulars>            </main_settings>            <product_page>                <enable>1</enable>                <max_count>0</max_count>                <title>Who bought this also bought</title>            </product_page>            <cart_page>                <enable>1</enable>                <max_count>0</max_count>                <title>Who bought this also bought</title>            </cart_page>            <slider>                <min_slides>1</min_slides>                <max_slides>10</max_slides>                <slide_width>170</slide_width>                <slide_margin>35</slide_margin>                <slide_mode>horizontal</slide_mode>                <infinite_loop>false</infinite_loop>            </slider>        </iwd_alsobought>    </default></config>